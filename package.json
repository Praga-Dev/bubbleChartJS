    "bubble chart",
    "chartjs",
    "chart.js",
    "visualization",
    "data visualization",
    "bubble graph",
    "canvas chart",
    "charts",
    "graph",
    "chart library",
    "chart plugin",
    "data plotting",
    "bubble chartjs"
  ]
}
